<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiSelectListPreference">
        <attr name="android:entries" />
        <attr name="android:entryValues" />
    </declare-styleable>

    <declare-styleable name="CustomAppBar">
               
        <attr name="background_colour" format="color" />
               
        <attr name="foreground_colour" format="color" />
        <attr name="app_bar_type" format="enum">
            <enum name="DISABLE_BLUR" value="0" />
            <enum name="ON_EXPANDED" value="50" />
            <enum name="FULL_BLUR" value="100" />
        </attr>
           
    </declare-styleable>
    <declare-styleable name="BlurView">
        <attr name="blurOverlayColor" format="color" />
    </declare-styleable>

    <declare-styleable name="MoreAppsCard">
        <attr name="CardTitle" format="string" />
        <attr name="AppsCategory" format="string" />
        <attr name="JsonName" format="string" />
    </declare-styleable>

    <declare-styleable name="TagView">
        <attr name="TagStyle" format="enum" >
            <enum name="Mono" value="0" />
            <enum name="Duo" value="1" />
        </attr>
        <attr name="MonoTagName" format="string" />
        <attr name="DualTagName0" format="string" />
        <attr name="DualTagName1" format="string" />
    </declare-styleable>

    <declare-styleable name="AdaptiveToolbar">
        <attr name="ToolbarStyle" format="enum" >
            <enum name="Home" value="0" />
            <enum name="Details" value="1" />
        </attr>
    </declare-styleable>
</resources>
